{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "prettier"
//    "airbnb",
//    "airbnb-typescript",
//    "plugin:prettier/recommended",
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "@typescript-eslint/lines-between-class-members": "off",
    "no-restricted-imports": [
      "error",
      {
        "patterns": ["@mui/*/*/*"]
      }
    ]
  }
}